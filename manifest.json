{
  "manifest_version": 13,
  "name": "Turkish Delight",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["wardrobe.js"]
    }
  ],
  "web_accessible_resources": [
    "edmond.js"
   ],
  "background": {
    "scripts": [
      "wardrobe.js",
      "edmond.js",
      "whitewitch.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "wardrobe.html"
  }
}
